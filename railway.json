{
  "build": {
    "builder": "DOCKERFILE"
  },
  "deploy": {
    "numReplicas": 1,
    "startCommand": "acme-dns -c /etc/acme-dns/config.cfg",
    "healthcheckPath": "/health",
    "port": 80
  },
  "env": {
    "ACMEDNS_DOMAIN": "acme.getfreeweb.site"
  },
  "volumes": [
    {
      "mountPath": "/data",
      "name": "acmedns_data"
    }
  ]
}

